{
    "name": "elife/annotations",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests"
        }
    },
    "require": {
        "php": "^7.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "aws/aws-sdk-php": "^3.38",
        "beberlei/assert": "^2.7",
        "elife/api-client": "^1.0@dev",
        "elife/api-problem": "^1.1",
        "elife/api-sdk": "^1.0@dev",
        "elife/bus-sdk": "^1.0@dev",
        "elife/content-negotiator": "^1.0",
        "elife/logging-sdk": "^1.0@dev",
        "elife/ping": "^1.0",
        "ezyang/htmlpurifier": "^4.9",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "guzzlehttp/psr7": "^1.4",
        "knplabs/console-service-provider": "^2.1",
        "league/commonmark": "^0.18.1",
        "pimple/pimple": "^3.2",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "silex/silex": "^2.2",
        "symfony/console": "^3.3",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/property-access": "^3.3",
        "symfony/serializer": "^3.3",
        "willdurand/negotiation": "^2.3"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "elife/api": "^2.0",
        "elife/api-validator": "^1.0@dev",
        "giorgiosironi/eris": "^0.12.1",
        "justinrainbow/json-schema": "^5.2",
        "mindplay/composer-locator": "^2.1",
        "phpunit/phpunit": "^6.5",
        "symfony/browser-kit": "^3.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.6"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.29"
        },
        "sort-packages": true
    }
}
