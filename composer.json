{
    "name": "elife/annotations",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests"
        },
        "files": [
            "src/Annotations/Serializer/CommonMark/functions.php"
        ]
    },
    "require": {
        "beberlei/assert": "^2.7",
        "cweagans/composer-patches": "^1.6",
        "elife/api-client": "^1.0@dev",
        "elife/api-problem": "^1.0",
        "elife/api-sdk": "^1.0@dev",
        "elife/bus-sdk": "^1.0@dev",
        "elife/content-negotiator": "^1.0",
        "elife/logging-sdk": "^1.0@dev",
        "elife/ping": "^1.0",
        "ezyang/htmlpurifier": "dev-master",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "knplabs/console-service-provider": "^2.1",
        "league/commonmark": "^0.17.0",
        "league/html-to-markdown": "^4.6@dev",
        "psr/container": "^1.0",
        "silex/silex": "^2.2",
        "symfony/property-access": "^3.3",
        "symfony/serializer": "^3.3",
        "willdurand/negotiation": "^2.3"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "elife/api": "dev-master",
        "elife/api-validator": "^1.0@dev",
        "giorgiosironi/eris": "^0.9.0",
        "mindplay/composer-locator": "^2.1",
        "mtdowling/jmespath.php": "^2.4",
        "phpunit/phpunit": "^5.7",
        "symfony/browser-kit": "^3.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.6"
    },
    "suggest": {
        "mtdowling/jmespath.php": "^2.4, to use the Result::search() method"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "league/html-to-markdown": {
                "Add newlines after code block": "https://github.com/thephpleague/html-to-markdown/pull/148.patch"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
